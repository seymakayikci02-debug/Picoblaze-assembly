;;detects if subtraction results are poritive or negative

	LOAD s0 17
	LOAD s1 15
	sub s0,s1
	
	TEST s0, 128     ; does s1 have MSB = 1?
	JUMP NZ, negative
	TEST s0, 128
	JUMP Z, positive

negative:
	LOAD s2,02
	LOAD s3,05
	JUMP done

positive:
	LOAD s2,03
	LOAD s4,06
	JUMP done
	
done:
	JUMP done
; else positive
